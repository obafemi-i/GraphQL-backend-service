{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["plugin:prettier/recommended", "eslint:recommended"],
    "plugins": [ "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "error",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "import/extensions": "off"
    }
}